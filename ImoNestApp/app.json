{
  "expo": {
    "name": "ImoNestApp",
    "slug": "ImoNestApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "imonest",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "plugins": [
        "expo-router"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hamzalabib.ImoNestApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "123652d9-5fc7-44ee-9871-ecddbb9a2011"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/123652d9-5fc7-44ee-9871-ecddbb9a2011"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "owner": "hamzalabib"
  }
}
